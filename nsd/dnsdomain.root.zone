;## dnsdomain.root authoritative zone

$ORIGIN dnsdomain.root.
$TTL 86400

@ IN SOA ns1.dnsdomain.root. webmaster.dnsdomain.root. (
                2011100501      ; serial
                28800           ; refresh
                7200            ; retry
                86400           ; expire
                86400           ; min TTL
                )

                NS              ns1.dnsdomain.root.
                MX      10      mail.dnsdomain.root.
